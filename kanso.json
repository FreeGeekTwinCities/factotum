{
    "name": "frontdesk",
    "version": "0.0.4",
    "description": "a prototype application to track 'front desk' info",
    "load": "lib/app",
    "modules": "lib",
    "attachments": "static",
    "dust": {
        "templates": "templates"
    },
    "duality": {
        "base_template": "base.html"
    },
    "git_info" : {
        "skip_uncommitted" : true
    },
    "less" : {
	"compress": true,
	"compile": ["static/css/main.less"],
	"remove_from_attachments": true
    },
    "dependencies": {
        "modules": null,
        "properties": null,
        "attachments": null,
        "settings": null,
        "duality": null,
        "dust": null,
        "duality-dust": null,
        "couchtypes": null,
	"less-precompiler": null,
	"bootstrap": null,
	"git-info": null
    }
}
